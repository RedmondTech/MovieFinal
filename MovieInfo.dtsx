          SQLTask:SqlStatementSource="USE DataSink&#xA;DROP TABLE IF EXISTS dbo.Amazon_Comedy&#xA;&#xA;CREATE TABLE dbo.Amazon_Comedy&#xA;(type nvarchar(max), title nvarchar(max), director nvarchar(max) , cast nvarchar(max), release_year int, listed_in nvarchar(max), description nvarchar(max),updatetime datetime)&#xA;&#xA;INSERT INTO dbo.Amazon_Comedy (type, title, director, cast, release_year, listed_in , description, updatetime)&#xA;SELECT&#xA;       [type]&#xA;      ,[title]&#xA;      ,[director]&#xA;      ,[cast]&#xA;  &#xA;      ,[release_year]&#xA;      ,[listed_in]&#xA;      ,[description]&#xA;&#x9;  ,getdate() updatetime&#xA;  FROM [Datasource].[dbo].[amazon_prime_titles]&#xA;&#xA;  WHERE Listed_IN LIKE '%Comedy%'&#xA;&#xA;USE DataSink&#xA;DROP TABLE IF EXISTS dbo.Amazon_Horror&#xA;&#xA;CREATE TABLE dbo.Amazon_Horror&#xA;(type nvarchar(max), title nvarchar(max), director nvarchar(max) , cast nvarchar(max), release_year int, listed_in nvarchar(max), description nvarchar(max),updatetime datetime)&#xA;&#xA;INSERT INTO dbo.Amazon_Horror (type, title, director, cast, release_year, listed_in , description, updatetime)&#xA;SELECT&#xA;       [type]&#xA;      ,[title]&#xA;      ,[director]&#xA;      ,[cast]&#xA;  &#xA;      ,[release_year]&#xA;      ,[listed_in]&#xA;      ,[description]&#xA;&#x9;  ,getdate() updatetime&#xA;  FROM [Datasource].[dbo].[amazon_prime_titles]&#xA;&#xA;  WHERE Listed_IN LIKE '%Horror%'&#xA;&#xA;  USE DataSink&#xA;DROP TABLE IF EXISTS dbo.Amazon_SciFi&#xA;&#xA;CREATE TABLE dbo.Amazon_SciFi&#xA;(type nvarchar(max), title nvarchar(max), director nvarchar(max) , cast nvarchar(max), release_year int, listed_in nvarchar(max), description nvarchar(max),updatetime datetime)&#xA;&#xA;INSERT INTO dbo.Amazon_SciFi (type, title, director, cast, release_year, listed_in , description, updatetime)&#xA;SELECT&#xA;       [type]&#xA;      ,[title]&#xA;      ,[director]&#xA;      ,[cast]&#xA;  &#xA;      ,[release_year]&#xA;      ,[listed_in]&#xA;      ,[description]&#xA;&#x9;  ,getdate() updatetime&#xA;  FROM [Datasource].[dbo].[amazon_prime_titles]&#xA;&#xA;  WHERE Listed_IN LIKE '%Sci%'&#xA;&#xA;USE DataSink&#xA;DROP TABLE IF EXISTS dbo.Amazon_Western&#xA;&#xA;CREATE TABLE dbo.Amazon_Western&#xA;(type nvarchar(max), title nvarchar(max), director nvarchar(max) , cast nvarchar(max), release_year int, listed_in nvarchar(max), description nvarchar(max),updatetime datetime)&#xA;&#xA;INSERT INTO dbo.Amazon_Western (type, title, director, cast, release_year, listed_in , description, updatetime)&#xA;SELECT&#xA;       [type]&#xA;      ,[title]&#xA;      ,[director]&#xA;      ,[cast]&#xA;  &#xA;      ,[release_year]&#xA;      ,[listed_in]&#xA;      ,[description]&#xA;&#x9;  ,getdate() updatetime&#xA;  FROM [Datasource].[dbo].[amazon_prime_titles]&#xA;&#xA;  WHERE Listed_IN LIKE '%western%'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
